##############
# ezmlm-weed #
##############

  prompt "ezmlm-weed:           "

  ${ECHO} "Subject: test" | ${EZBIN}/ezmlm-weed || \
	fatal "failed to accept good message"
  ${ECHO} "Subject: success notice" | ${EZBIN}/ezmlm-weed >/dev/null 2>&1 && \
	fatal "failed to reject bad message"

  ${ECHO} "OK"
